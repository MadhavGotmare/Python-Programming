import math
import os
import random
import re
import sys



if __name__ == '__main__':
    n = int(input().strip())
    range_1 = range(2, 5)
    range_2 = range(6, 20)
    range_3 = range(20, 100)
    
        
    if (n%2) == 0 and n in range_1:
        print("Not Weird")
        
    elif (n%2) == 0 and n in range_2:
        print("Weird")
        
    elif (n%2) == 0 and n > 20:
        print("Not Weird")
        
    else:
        print("Weird")
